{
  "name": "zazu-open-jira-issue",
  "version": "1.0.0",
  "description": "simple zazu plugin to directly open Jira issues in your browser by issue key",
  "main": "zazu.js",
  "private": true,
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marco-streng/zazu-open-jira-issue.git"
  },
  "keywords": [
    "zazu",
    "jira"
  ],
  "author": "Marco Streng",
  "license": "MIT",
  "devDependencies": {
    "jest": "^22.3.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
